<h1>Listing uservideoassignments</h1>

<table>
  <tr>
    <th>Video</th>
    <th>User</th>
    <th>Tagged by user</th>
  </tr>

<% @uservideoassignments.each do |uservideoassignment| %>
  <tr>
    <td><%=h uservideoassignment.video_id %></td>
    <td><%=h uservideoassignment.user_id %></td>
    <td><%=h uservideoassignment.tagged_by_user_id %></td>
    <td><%= link_to 'Show', uservideoassignment %></td>
    <td><%= link_to 'Edit', edit_uservideoassignment_path(uservideoassignment) %></td>
    <td><%= link_to 'Destroy', uservideoassignment, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New uservideoassignment', new_uservideoassignment_path %>