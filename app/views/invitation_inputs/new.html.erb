<h1>New invitation_input</h1>

<% form_for(@invitation_input) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :input %><br />
    <%= f.text_field :input %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', invitation_inputs_path %>