<h1>New picturecomment</h1>

<% form_for(@picturecomment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :picture_id %><br />
    <%= f.text_field :picture_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', picturecomments_path %>